{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "quotemark": [
          true,
          "single"
      ],
      "indent": [
          true
      ],
      "interface-name": [
          false
  ],
  "trailing-comma": false,
  "arrow-parens": false,
      "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
