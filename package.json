    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^11.13.6",
    "@types/webpack": "^4.4.27",
    "@types/webpack-dev-server": "^3.1.5",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.5",
