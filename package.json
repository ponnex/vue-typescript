{
  "name": "vue-typescript",
  "version": "0.1.0",
  "description": "Vue project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "node_modules/.bin/webpack-dev-server --hot --env.mode=development",
    "prebuild": "npm run clean && npm run build:sfc",
    "build": "npm run rm && npm run tsc:webpack && node_modules/.bin/webpack --env.mode=production",
    "tsc:webpack": "node_modules/.bin/tsc webpack.config.ts",
    "rm": "rimraf webpack.config.js",
    "generate": "node cli/command.js generate",
    "build:sfc": "node cli/sfc.js",
    "clean": "rimraf ./src/**/*.component.vue ./src/*.prod.ts"
  },
  "keywords": [
    "Vue",
    "Typescript"
  ],
  "author": "geoff.diaz@curiouslab.sg",
  "license": "ISC",
  "devDependencies": {
