    "ts-node": "^8.1.0",
    "typescript": "^3.4.4",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "devConfig": {
    "port": 8080,
    "host": "0.0.0.0"
  },
  "vendors": [
    "lodash"
  ],
